docker run --name mysql -p 3306:3306 -e MYSQL_ROOT_PASSWORD=password -e MYSQL_DATABASE=carddb -e MYSQL_USER=card -e MYSQL_PASSWORD=password -d mysql:5.6

docker run -p 5432:5432 --name postgres -e POSTGRES_PASSWORD=password -e POSTGRES_USER=member -e POSTGRES_DB=memberdb -d -v /var/lib/postgresql/data postgres